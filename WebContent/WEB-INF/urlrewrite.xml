<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE urlrewrite  
  
        PUBLIC "-//tuckey.org//DTD UrlRewrite 3.2//EN"  
  
        "http://tuckey.org/res/dtds/urlrewrite3.2.dtd">

<urlrewrite>  
    <rule>  
       <from>article_([0-9])_([0-9]).html</from> 
        <to  type="forward">showArticle.action?id=$1&amp;state=$2</to>  
    </rule>  
  
</urlrewrite>  
