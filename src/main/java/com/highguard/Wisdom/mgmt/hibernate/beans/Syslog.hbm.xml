<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.highguard.Wisdom.mgmt.hibernate.beans.Syslog"
		table="syslogs">
		<id name="id" type="java.lang.Integer" unsaved-value="null">
			<generator class="native" />
		</id>
		<property name="uid" type="java.lang.Integer" />
		<property name="username" type="java.lang.String" />
		<property name="level" type="java.lang.String" />
		<property name="msg" type="java.lang.String" />
		<property name="classname" type="java.lang.String" />
		<property name="createtime" type="java.lang.String" />
		<property name="method" type="java.lang.String" />
	</class>
</hibernate-mapping>