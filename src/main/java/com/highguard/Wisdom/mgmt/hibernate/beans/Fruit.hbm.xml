<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.highguard.Wisdom.mgmt.hibernate.beans.Fruit"
		table="fruit">
		<id name="id" type="java.lang.Integer" unsaved-value="null">
			<generator class="native" />
		</id>	
		<property name="num" type="java.lang.String" />
		<property name="category" type="java.lang.String" />
		<property name="productname" type="java.lang.String" />
		<property name="grade" type="java.lang.String" />
		<property name="price" type="java.lang.String" />
		<property name="weight" type="java.lang.String" />
		<property name="place" type="java.lang.String" />
		<property name="imageurl" type="java.lang.String" />
		<property name="texture" type="java.lang.String" />
		<property name="description" type="java.lang.String" />
		
	</class>
</hibernate-mapping>