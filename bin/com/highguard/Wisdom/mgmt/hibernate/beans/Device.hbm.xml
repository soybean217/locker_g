<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.highguard.Wisdom.mgmt.hibernate.beans.Device"
		table="device">
		<id name="id" type="java.lang.Integer" unsaved-value="null">
			<generator class="native" />
		</id>
		<property name="num" type="java.lang.String" />
		<property name="location" type="java.lang.String" />
		<property name="cellnum" type="java.lang.String" />
		<property name="status" type="java.lang.String" />
		<property name="comment" type="java.lang.String" />
		<property name="name" type="java.lang.String" />
		<property name="managerid" type="java.lang.Integer" />
		<property name="wximage" type="java.sql.Blob" />
	</class>
</hibernate-mapping>