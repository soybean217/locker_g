<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.highguard.Wisdom.mgmt.hibernate.beans.AppVersion"
		table="appversion">
		<id name="id" type="java.lang.Integer" unsaved-value="null">
			<generator class="native" />
		</id>
		<property name="appVersion" column="appversion" type="java.lang.String" />
		<property name="version" column="version"  type="java.lang.Integer" />
		<property name="fileName" column="filename" type="java.lang.String" />
		<property name="createTime" column="create_time" type="java.sql.Timestamp" />
	</class>
</hibernate-mapping>